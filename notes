tab-shifter{
  alt+shift+] - shift tab right
    alt+shift+[ - shift tab left
    alt+shift+P - shift tab up
    alt+shift+; - shift tab down
    ctrl+alt+[ - stretch split left
    ctrl+alt+] - stretch split right
    alt+shift+M - maximize/restore split

}
